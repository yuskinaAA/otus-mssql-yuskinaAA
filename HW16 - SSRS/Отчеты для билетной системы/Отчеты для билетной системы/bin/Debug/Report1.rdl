  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@dateStart">
            <Value>=Parameters!dateStart.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dateEnd">
            <Value>=Parameters!dateEnd.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT  ow.name, SUM(o.amount) [сумма]
FROM Purchasing.Orders AS o 
INNER JOIN Directory.Events_time AS et ON et.id = o.event_time_id 
INNER JOIN Directory.Events AS e ON e.id = et.event_id
INNER JOIN Directory.Owners AS ow ON ow.id = e.owner_id
WHERE o.order_date BETWEEN DATEADD(HOUR,0,@dateStart) AND DATEADD(SECOND,86399,@dateEnd)
GROUP BY ow.name
ORDER BY ow.name</CommandText>
      </Query>
      <Fields>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="сумма">
          <DataField>сумма</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
